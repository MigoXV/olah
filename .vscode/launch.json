{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debugpy: server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/olah/server.py",
            "console": "integratedTerminal",
            "args": ["-c", "${workspaceFolder}/assets/full_configs.toml"],
            "justMyCode": false
        },
        {
            "name": "debugpy: model-bin",
            "type": "debugpy",
            "request": "launch",
            "module": "olah.commands.app",
            "console": "integratedTerminal",
            "args": ["model-bin", "--path", "${workspaceFolder}/model-bin"],
            "justMyCode": false
        }
    ]
}